{
  "name": "pro-time-r3-ui",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.conf.json",
    "start-local": "ng serve --proxy-config ./proxy.conf.local.json",
    "start-server": "json-server --watch db.json --routes db.routes.json",
    "build": "ng build --prod --source-map false --output-path ../ProTime-R3-backend/src/main/webapp/app --base-href .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "openapi-generator generate -g typescript-angular -o src/app/api -i ./openapi.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.3",
    "@angular/common": "~9.0.0-rc.3",
    "@angular/compiler": "~9.0.0-rc.3",
    "@angular/core": "~9.0.0-rc.3",
    "@angular/forms": "~9.0.0-rc.3",
    "@angular/platform-browser": "~9.0.0-rc.3",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.3",
    "@angular/router": "~9.0.0-rc.3",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "jwt-decode": "^2.2.0",
    "ng-dm-libs": "git+https://github.com/dominikmathmann/ng-dm-libs.git",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.5",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular/cli": "~9.0.0-rc.3",
    "@angular/compiler-cli": "~9.0.0-rc.3",
    "@angular/language-service": "~9.0.0-rc.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.2",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
